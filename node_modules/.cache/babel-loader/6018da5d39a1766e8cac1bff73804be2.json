\r\n          //const img = document.querySelector(\"#tagImagem\");\r\n          //console.log(res);\r\n          if(this.usuario.pontuacao > 5) {\r\n            this.acima = true;\r\n          } else if (this.usuario.pontuacao == 5) {\r\n            this.media = true;\r\n          } else if (this.usuario.pontuacao < 5){\r\n            this.abaixo = true;\r\n          }\r\n        });\r\n    },\r\n    initialize() {\r\n      this.$http\r\n        .get(\"http://localhost:8091/arquivo/\" + this.usuarioId)\r\n        .then((res) => {\r\n          this.usuario = res.data;\r\n          console.log(\"aqui:\" , this.usuario);\r\n          \r\n          \r\n          //const img = document.querySelector(\"#tagImagem\");\r\n          //console.log(res);\r\n        });\r\n          console.log(this.abaixo);\r\n          console.log(this.media);\r\n          console.log(this.acima);\r\n    },\r\n    verificarCodigo(event) {\r\n      if (event.which === 13) {\r\n        if (this.srcSet == this.inputCodigo) {\r\n          this.respostCorreta();\r\n          this.pontuacao += 1;\r\n          this.indice++;\r\n          this.initialize();\r\n          if (this.arquivos.length == this.indice) {\r\n            this.$router.push(`/atividade-encerrada/${this.atividadeId}/${this.usuarioIdPath}/${this.pontuacao}`);\r\n          }\r\n        } else {\r\n          this.respostErrada();\r\n          this.pontuacao += 0;\r\n          this.indice++;\r\n          this.initialize();\r\n          if (this.arquivos.length == this.indice) {\r\n            this.$router.push(\"/atividade-encerrada\");\r\n          }\r\n        }\r\n        this.inputCodigo = \"\";\r\n        console.log(this.pontuacao);\r\n      }\r\n    },\r\n    blockSubmit(event) {\r\n      event.preventDefault();\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\nh1, h2, h3 {\r\n  font-weight: normal;\r\n  text-align: center;\r\n}\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\nli {\r\n  display: inline-block;\r\n  margin: 0 10px;\r\n}\r\na {\r\n  color: #42b983;\r\n}\r\nh1.h1titulo{\r\n  color: #42b983;\r\n}\r\n.bodyPage{\r\n  margin-top: 100px;\r\n}\r\n</style>"],"sourceRoot":"src/page"}]}